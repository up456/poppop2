{
  "name": "poppop2",
  "version": "1.0.12",
  "description": "React component that makes children elements pop with animation effects",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "react",
    "animation",
    "effect",
    "pop",
    "component"
  ],
  "author": "fabbro",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^19.0.1",
    "@vitejs/plugin-react": "^4.2.0",
    "jsdom": "^25.0.1",
    "tsup": "^8.3.5",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^2.1.8"
  }
}
